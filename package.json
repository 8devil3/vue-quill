{
  "name": "@vueup/quill",
  "version": "0.1.2",
  "description": "Quill Editor for Vue 3 plugin or component.",
  "keywords": [
    "vue 3 quill plugin",
    "vue 3 quill component",
    "vue 3 quill editor",
    "vue 3 text editor",
    "vue 3 rich text editor",
    "vue 3 web editor",
    "vue 3 editor",
    "vue 3 wysiwyg",
    "vue 3 wysiwyg editor"
  ],
  "homepage": "https://vueup.github.io/vueup-quill/",
  "bugs": {
    "url": "https://github.com/vueup/vueup-quill/issues",
    "email": "luthfimasruri@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Ahmad Luthfi Masruri",
    "email": "luthfimasruri@gmail.com"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://paypal.me/bledex"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vueup/vueup-quill.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*.css",
    "dist/*.js"
  ],
  "main": "./dist/quill.umd.js",
  "module": "./dist/quill.es.js",
  "browser": "dist/quill.umd.js",
  "unpkg": "dist/quill.umd.js",
  "jsdelivr": "dist/quill.umd.js",
  "exports": {
    ".": {
      "import": "./dist/quill.es.js",
      "require": "./dist/quill.umd.js"
    },
    "./": "./"
  },
  "scripts": {
    "lib:build": "vite build",
    "demo:dev": "cd demo && vite serve",
    "demo:build": "cd demo && vite build",
    "demo:serve": "cd demo && vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "theme:snow": "stylus -w ./src/assets/snow.styl -o ./src/themes/quill.snow2.css"
  },
  "dependencies": {
    "quill": "^1.3.7",
    "vue": "^3.0.7",
    "vue-prism-component": "^2.0.0"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/node": "^14.14.34",
    "@types/quill": "^2.0.5",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.7",
    "autoprefixer": "^10.2.5",
    "csso": "^4.2.0",
    "postcss": "^8.2.8",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-svg-import": "^1.5.1",
    "tailwindcss": "^2.0.3",
    "types-quill-delta": "^1.0.0",
    "typescript": "^4.2.3",
    "vite": "^2.0.5",
    "vitepress": "^0.12.2"
  }
}
