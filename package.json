{
  "name": "@vueup/quill",
  "version": "0.0.0-development",
  "description": "Quill Editor for Vue 3 plugin or component.",
  "keywords": [
    "vue 3 quill plugin",
    "vue 3 quill component",
    "vue 3 quill editor",
    "vue 3 text editor",
    "vue 3 rich text editor",
    "vue 3 web editor",
    "vue 3 editor",
    "vue 3 wysiwyg",
    "vue 3 wysiwyg editor"
  ],
  "homepage": "https://vueup.github.io/vueup-quill/",
  "bugs": {
    "url": "https://github.com/vueup/vueup-quill/issues",
    "email": "luthfimasruri@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Ahmad Luthfi Masruri",
    "email": "luthfimasruri@gmail.com"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://paypal.me/bledex"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vueup/vueup-quill.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*.css",
    "dist/*.js"
  ],
  "main": "./dist/quill.umd.js",
  "module": "./dist/quill.es.js",
  "browser": "dist/quill.umd.js",
  "unpkg": "dist/quill.umd.js",
  "jsdelivr": "dist/quill.umd.js",
  "exports": {
    ".": {
      "import": "./dist/quill.es.js",
      "require": "./dist/quill.umd.js"
    },
    "./": "./"
  },
  "scripts": {
    "lib:build": "vite build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:serve": "cd docs && npm run serve",
    "theme:dev": "stylus -w ./themes/assets/snow.styl -o ./themes/css/quill.snow.css",
    "theme:build": "stylus ./themes/assets/snow.styl -o ./themes/css/quill.snow.css"
  },
  "dependencies": {
    "quill": "^1.3.7",
    "vue": "^3.0.7"
  },
  "devDependencies": {
    "@semantic-release/exec": "^5.0.0",
    "@types/node": "^14.14.35",
    "@types/quill": "^2.0.5",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.7",
    "csso": "^4.2.0",
    "quill-delta": "^4.2.2",
    "rollup-plugin-copy": "^3.4.0",
    "semantic-release": "^17.4.2",
    "stylus": "^0.54.8",
    "typescript": "^4.2.3",
    "vite": "^2.1.2"
  }
}
