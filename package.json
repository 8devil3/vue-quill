  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/quill": "^2.0.5",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-chunks": "^2.0.2",
    "types-quill-delta": "^1.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.1",
    "vitepress": "^0.12.2"
