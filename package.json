{
  "name": "@vueup/vue-quill",
  "version": "0.0.0-development",
  "description": "Quill Editor for Vue 3 plugin or component.",
  "keywords": [
    "vue 3 quill plugin",
    "vue 3 quill component",
    "vue 3 quill editor",
    "vue 3 text editor",
    "vue 3 rich text editor",
    "vue 3 web editor",
    "vue 3 editor",
    "vue 3 wysiwyg",
    "vue 3 wysiwyg editor"
  ],
  "homepage": "https://vueup.github.io/vue-quill/",
  "bugs": {
    "url": "https://github.com/vueup/vue-quill/issues",
    "email": "luthfimasruri@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Ahmad Luthfi Masruri",
    "email": "luthfimasruri@gmail.com"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://paypal.me/bledex"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vueup/vue-quill.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/vue-quill.cjs.js",
  "module": "dist/vue-quill.es.js",
  "browser": "dist/vue-quill.umd.js",
  "unpkg": "dist/vue-quill.umd.js",
  "jsdelivr": "dist/vue-quill.umd.js",
  "types": "dist/vue-quill.d.ts",
  "scripts": {
    "lib:dev": "npm run theme:dev && rollup -c rollup.config.ts -w",
    "lib:build": "npm run theme:build && rollup -c rollup.config.ts && npm run lib:types",
    "lib:types": "tsc --emitDeclarationOnly true && npx dts-bundle --configJson dts-bundle.json",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:serve": "cd docs && npm run serve",
    "theme:dev": "stylus -w ./src/themes/assets/snow.styl -o ./src/themes/css/vue-quill.snow.css",
    "theme:build": "stylus ./src/themes/assets/snow.styl -o ./src/themes/css/vue-quill.snow.css"
  },
  "dependencies": {
    "quill": "^1.3.7",
    "vue": "^3.0.7"
  },
  "devDependencies": {
    "@semantic-release/exec": "^5.0.0",
    "@types/node": "^14.14.35",
    "@types/quill": "^2.0.5",
    "csso": "^4.2.0",
    "dts-bundle": "^0.7.3",
    "lodash.camelcase": "^4.3.0",
    "quill-delta": "^4.2.2",
    "rollup": "^2.42.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.2",
    "stylus": "^0.54.8",
    "typescript": "^4.2.3"
  }
}